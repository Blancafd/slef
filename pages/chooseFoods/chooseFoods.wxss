/* pages/menus/choose-foods/choose-foods.wxss */

.clear {
  clear: both;
}

.header {
  padding: 0 30rpx;
  height: 80rpx;
  position: fixed;
  width: 100%;
  top: 0px;
  z-index: 100;
  background-color: #fff;
}

image {
  display: inline-block;
}

.header_left_img {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}

.header_left {
  position: relative;
  float: left;
  width: 75rpx;
  height: 80rpx;
}

.header_right {
  float: left;
  height: 80rpx;
  position: relative;
  width: 424rpx;
}

.header_right_cont {
  border-radius: 28rpx;
  background-color: #f5f5f5;
  height: 60rpx;
  width: 424rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}

.serch {
  height: 60rpx !important;
  padding-left: 20rpx;
  font-size: 26rpx;
  color: #333;
}

.serch_img {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 20rpx;
}

/* 轮播图 */

.binner {
  padding: 0 40rpx;
  margin-top: 30rpx;
}

.carouselImg {
  width: 670rpx;
  height: 310rpx;
}

.binner_swiper {
  height: 310rpx;
}

.wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 5rpx;
  float: left;
  text-align: center;
}

.wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: white;
  border-radius: 15rpx;
  border: 0rpx;
  opacity: 1;
}

.wx-swiper-dot.wx-swiper-dot {
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background: white;
  opacity: 1;
}

.wx-swiper-dot.wx-swiper-dot-active {
  width: 72rpx;
  height: 20rpx;
  border-radius: 10rpx;
  opacity: 1;
}

/* 菜品种类 */

.menu_type {
  margin-top: 40rpx;
  height: 176rpx;
  background-color: #f6f6f6;
}

.menu_scroll {
  display: flex;
  white-space: nowrap;
  height: 176rpx;
}

.menu_scroll_view {
  width: 160rpx;
  height: 160rpx;
  display: inline-block;
  background-color: #fff;
  margin-right: 4rpx;
  position: relative;
}

.menu_scroll_img {
  width: 100rpx;
  height: 100rpx;
  position: relative;
  left: 50%;
  transform: translateX(-50%);
  top: 10rpx;
}

.menu_scroll_title {
  text-align: center;
  color: #333;
  font-size: 28rpx;
}

.menu_scroll_active {
  background-color: #f6f6f6;
  font-weight: 600;
}

/* .菜单列表  */

.menu_list {
  padding: 0 40rpx;
}

.list {
  padding-top: 60rpx;
  display: flex;
  position: relative;
}

.list:last-child {
  padding-bottom: 100rpx;
}

.list_left {
  flex: 1;
  position: relative;
}

.nofood {
  width: 200rpx;
  height: 46rpx;
  position: absolute;
  bottom: 10rpx;
  z-index: 99;
  left: 0px;
}

.color {
  color: #999 !important;
}

.list_right {
  flex: 2;
  padding-left: 10rpx;
}

.list_left_img {
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
}

.list_right_line {
  display: flex;
}

.list_right_title {
  color: #333;
  font-size: 36rpx;
  font-weight: Bold;
}

.list_right_yu {
  width: 112rpx;
  height: 34rpx;
  background-color: #fcd7d7;
  font-size: 24rpx;
  border-radius: 6rpx;
  color: #ef1212;
  text-align: center;
  font-weight: bold;
  margin-left: 16rpx;
  margin-top: 12rpx;
  line-height: 34rpx;
}

.list_right_cont {
  color: #999;
  font-size: 24rpx;
  padding-top: 8rpx;
  height: 68rpx;
}

.price {
  padding-top: 10rpx;
}

.price_left {
  float: left;
  width: 270rpx;
  height: 50rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.price_right {
  float: right;
  display: flex;
  padding-top: 10rpx;
}

.price_left_yuan {
  color: #e61212;
  font-size: 24rpx;
}

.price_left_price {
  color: #e61212;
  font-size: 36rpx;
  font-weight: Bold;
}

.price_left_ke {
  color: #999;
  font-size: 24rpx;
}

.price_right_img {
  width: 54rpx;
  height: 54rpx;
}

.price_right_number {
  color: #333;
  font-size: 32rpx;
  padding: 0 14rpx;
  text-align: center;
  line-height: 20rpx;
}

::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}

.guige {
  background-color: #fbaf02;
  border-radius: 20rpx;
  width: 100rpx;
  height: 46rpx;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  line-height: 46rpx;
  margin-top: 4rpx;
}

/*底部 */

.footer {
  position: fixed;
  bottom: 0px;
  width: 100%;
  height: 100rpx;
  background-color: #555;
  display: flex;
  z-index: 999;
}

.shop {
  position: relative;
  width: 140rpx;
}

.footer_img {
  width: 120rpx;
  height: 120rpx;
  position: absolute;
  top: -40%;
  left: 40rpx;
  z-index: 99;
}

.footer_yuan {
  font-size: 26rpx;
  color: #fff;
  background-color: #e61212;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 40rpx;
  position: absolute;
  top: -30%;
  left: 92%;
  z-index: 999;
}

.footer_price {
  width: 320rpx;
}

.footer_price_yuan {
  color: #fff;
  font-size: 28rpx;
  line-height: 100rpx;
  padding-left: 60rpx;
}

.footer_price_num {
  color: #fff;
  font-size: 36rpx;
  line-height: 100rpx;
  font-weight: bold;
}

.jiesuan {
  background-color: #fbaf02;
  color: #fff;
  font-size: 36rpx;
  font-weight: 500;
  text-align: center;
  line-height: 100rpx;
  width: 210rpx;
  position: absolute;
  right: 0px;
}

/* 为选购 */

.footer_none {
  color: #d8d8d8;
  font-size: 36rpx;
  line-height: 100rpx;
  padding-left: 60rpx;
}

/* 遮罩层 */

.mask {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #000;
  z-index: 1;
  top: 0;
  left: 0;
  opacity: 0.3;
}

.displaynone {
  display: none;
}

.displayblock {
  display: block;
  z-index: 999;
}

.choose_guige {
  position: fixed;
  background-color: #fff;
  color: #393942;
  top: 50%;
  width: 85%;
  left: 50%;
  transform: translate(-50%, -50%);
  border-radius: 16rpx;
  z-index: 999;
}

.choose_guige_left {
  float: left;
  padding-top: 18rpx;
}

.choose_guige_title {
  color: #333;
  font-size: 38rpx;
  font-weight: bold;
}

.choose_guige_tip {
  color: #888;
  font-size: 28rpx;
}

.choose_guige_close {
  float: right;
  padding-top: 34rpx;
}

.close_img {
  width: 36rpx;
  height: 36rpx;
}

.guige_list {
  display: flex;
  height: 56rpx;
  padding: 0 40rpx;
  padding-bottom: 46rpx;
}

.guige_list_title {
  font-size: 28rpx;
  color: #333;
  line-height: 56rpx;
  padding-right: 20rpx;
}

.clear {
  clear: both;
}

.guige_list_btn {
  color: #fbaf02;
  font-size: 28rpx;
  border-radius: 28rpx;
  border: 2rpx solid #fbaf02;
  height: 56rpx;
  width: 124rpx;
  line-height: 56rpx;
  text-align: center;
  margin-left: 20rpx;
}

.choose {
  background-color: #fbaf02;
  color: #fff;
}

.guige_footer {
  height: 88rpx;
  background-color: #f6f6f6;
  padding: 0 40rpx;
  border-radius: 0px 0px 16rpx 16rpx;
  display: flex;
  position: relative;
}

.guige_footer_qian {
  font-size: 24rpx;
  line-height: 88rpx;
  color: #e61212;
}

.guige_footer_money {
  font-size: 36rpx;
  line-height: 88rpx;
  color: #e61212;
  font-weight: bold;
}

.guige_footer_weight {
  font-size: 24rpx;
  line-height: 88rpx;
  color: #888;
}

.gocanpan {
  width: 242rpx;
  height: 60rpx;
  background-color: #fbaf02;
  border-radius: 30rpx;
  position: absolute;
  top: 50%;
  right: 40rpx;
  transform: translateY(-50%);
  color: #fff;
  line-height: 60rpx;
  font-size: 28rpx;
}

.gocanpan_img {
  width: 40rpx;
  height: 40rpx;
  position: relative;
  top: 50%;
  transform: translateY(-50%);
  padding-left: 36rpx;
}

.footShop {
  position: fixed;
  bottom: 100rpx;
  width: 100%;
  z-index: 9;
}

.footShop_line1 {
  height: 80rpx;
  background-color: #e6e6e6;
  z-index: 9;
  font-size: 28rpx;
  color: #333;
  line-height: 80rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
}

.footShop_line1_left {
  float: left;
}

.footShop_line1_right {
  float: right;
  position: relative;
}

.footShop_line3 {
  height: 28rpx;
  background-color: #f1f1f1;
  z-index: -1;
}

.footShop_right_img {
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  left: -35%;
  top: 50%;
  transform: translateY(-50%);
}

.footShop_line2 {
  background-color: #fff;
  line-height: 134rpx;
}

.footShop_list {
  display: flex;
  line-height: 134rpx;
  height: 134rpx;
  border-bottom: 2rpx solid #f8f8f8;
  padding-left: 40rpx;
  padding-right: 40rpx;
}

.footShop_list_name {
  flex: 4;
  color: #333;
  font-size: 34rpx;
}

.footShop_list_price {
  flex: 3;
  text-align: right;
}

.footShop_list_num {
  flex: 2;
  display: flex;
  float: right;
  line-height: 134rpx;
  padding-left: 70rpx;
  padding-right: 20rpx;
  height: 134rpx;
}

.footShop_list_qian {
  font-size: 26rpx;
  color: #e61212;
}

.footShop_list_priceNum {
  font-size: 34rpx;
  color: #e61212;
  font-weight: blod;
}

.footShop_list_Num2 {
  color: #333;
  font-size: 32rpx;
  padding: 0 14rpx;
  text-align: center;
}

.footShop_list_img {
  width: 54rpx;
  height: 54rpx;
  position: relative;
  top: 50%;
  transform: translateY(-50%);
}

/* 打烊 */

.rest {
  position: fixed;
  top: 0px;
  height: 512rpx;
  background-color: #fff;
  width: 100%;
  z-index: 999;
  border-radius: 0px 0px 16rpx 16rpx;
}

.rest_img {
  width: 458rpx;
  height: 346rpx;
  position: absolute;
  right: 0px;
  bottom: 30rpx;
  z-index: -1;
}

.rest_line1 {
  color: #333;
  font-size: 44rpx;
  font-weight: bold;
  text-align: center;
  padding-top: 140rpx;
  padding-bottom: 40rpx;
}

.rest_line2 {
  color: #666;
  font-size: 24rpx;
  text-align: center;
}

.kong2 {
  background-color: rgba(216, 216, 216);
  margin: 0 50rpx;
  height: 40rpx;
  position: absolute;
  width: 87%;
  z-index: 99;
  top: 488rpx;
  border-radius: 20rpx;
}

.leftBottom {
  border-bottom-left-radius: 30rpx;
}

.rightBottom {
  border-bottom-right-radius: 30rpx;
}
