border-radius: 140rpx;
background: #fff;
position: absolute;
top:-20rpx;
z-index: -1;
opacity: 0.5;
animation: viewlinear 2s linear infinite;


}

@keyframes viewlinear {
  0%{
    transform:scale(1);
  }

   25%{
    transform:scale(0.6);
  }
  50%{
    transform:scale(0.7);
  }
  100%{
    transform:scale(1);
  }

}


.box2{
  width: 140rpx;
  height: 140rpx;
  padding: 40rpx;
  border-radius: 140rpx;
  background: #fff;
  position: absolute;
  z-index: -21;
  opacity: 0.2;
  top: -40rpx;
  left: -20rpx;
 animation: scale 3s linear infinite;
}

@keyframes scale {
   0%{
    transform:scale(1);
  }

   25%{
    transform:scale(0.6);
  }
  50%{
    transform:scale(0.7);
  }
  100%{
    transform:scale(1);
  }

}

