@keyframes viewscale {
 
  0%   {transform:translate(0rpx);}
	25%   {transform:translate(-10rpx);}
	50%   {transform:translate(0rpx);}
	75%  {transform:translate(-10rpx);}
	100%  {transform:translate(0rpx);}

}

