
.head{
  display: flex;
  align-items: center;
  padding-left: 30rpx
}
.back{
  width: 48rpx;
  height: 48rpx;
  margin-right: 44rpx
}
.back>image{
  width: 100%;
  height: 100%
}
.search{
  background: #F5F5F5;
  width: 424rpx;
  height: 56rpx;
  border-radius: 28rpx;
  display: flex;
  align-items: center;
  padding: 0rpx 20rpx;
  box-sizing: border-box;
  justify-content: space-between
}
.search>input{
  font-size: 26rpx
}
.searchImg{
  width: 36rpx;
  height: 36rpx
}
.searchImg>image{
  width: 100%;
  height: 100%
}
.title{
margin-left: 40rpx;
margin-top: 40rpx

}
.storelist{
  
  width: 670rpx;
  height: 160rpx;
  border-radius:16rpx;
  box-shadow:2rpx 2rpx 8rpx rgba(0,0,0,0.12);
  margin: 40rpx auto 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0rpx 30rpx;
  box-sizing: border-box
}
.enter{
  width: 22rpx;
  height: 22rpx
}
.enter>image{
  width: 100%;
  height: 100%
}

.listDetail>view{
  display: flex;
  justify-content: space-between
}
.addetail{
  
  display: flex;
  justify-content: space-between
}
.listDetail>view:nth-of-type(2){
  font-size: 24rpx;
  color: #999;
  display: flex;
  justify-content: space-between;
  margin-top: 16rpx;
  width: 550rpx
}
.address>view:nth-of-type(1){
  font-size: 32rpx;
  color: #333
}
.address>view:nth-of-type(2){
  font-size: 32rpx;
  color: #FBAF02
}
.textdetail{
  font-size: 20rpx;
  margin-left: 50rpx
}